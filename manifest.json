{
    "name": "Typing game",
    "description": "Simpe typing game.",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "64": "/images/icon-32x32.png",
        "128": "/images/icon-64x64.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs"
    ],
    "action": {
        "default_icon": {
            "64": "/images/icon-32x32.png",
            "128": "/images/icon-64x64.png"
        },
        "default_title": "Typing game",
        "default_popup": "popup.html"
    },
    "web_accessible_resources": [
        {
            "resources": ["/images/*"],
            "matches": [ "https://*/*" ]
        }
    ]
}